{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "import-spacing": false,
    "indent": [true, "tabs"],
    "member-access": false,
    "no-bitwise": false,
    "no-console": false,
    "only-arrow-functions": false,
    "prefer-const": false,
    "quotemark": [true, "single"],
    "one-variable-per-declaration": false,
    "radix": false,
    "semicolon": false,
    "space-before-function-paren": false,
    "trailing-comma": false
  },
  "rulesDirectory": []
}
